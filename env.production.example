# Configuration Production - DocuSense AI
# ⚠️ NE JAMAIS COMMITER CE FICHIER DANS GIT

# Base de données PostgreSQL
POSTGRES_PASSWORD=your_secure_postgres_password_here
DATABASE_URL=postgresql://docusense_user:your_secure_postgres_password_here@postgres:5432/docusense

# Cache Redis
REDIS_PASSWORD=your_secure_redis_password_here
REDIS_URL=redis://:your_secure_redis_password_here@redis:6379

# Sécurité
SECRET_KEY=your_very_long_and_secure_secret_key_here_minimum_32_characters
JWT_SECRET_KEY=your_jwt_secret_key_here_different_from_secret_key

# Environnement
ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=INFO

# API Configuration
API_V1_STR=/api
PROJECT_NAME=DocuSense AI
VERSION=1.0.0

# CORS
BACKEND_CORS_ORIGINS=["https://yourdomain.com", "https://www.yourdomain.com"]

# Email (optionnel)
SMTP_TLS=true
SMTP_PORT=587
SMTP_HOST=smtp.gmail.com
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password

# Monitoring
SENTRY_DSN=your_sentry_dsn_here

# Backup
BACKUP_ENABLED=true
BACKUP_RETENTION_DAYS=30
